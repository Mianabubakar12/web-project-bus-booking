<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Bus Seat Layout</title>
  <link rel="stylesheet" href="style.css">
</head>
<body>
  <!-- Navbar -->
  <div id="navbar"></div>

  <!-- Content Area -->
  <div class="content">
    <div class="seat-grid">
      <!-- Left Side Seats -->
      <div class="seats left-side">
        <div class="seat">1L</div>
        <div class="seat">2L</div>
        <div class="seat">3L</div>
        <div class="seat">4L</div>
        <div class="seat">5L</div>
        <div class="seat">6L</div>
        <div class="seat">7L</div>
        <div class="seat">8L</div>
        <div class="seat">9L</div>
        <div class="seat">10L</div>
        <div class="seat">11L</div>
        <div class="seat">12L</div>
        <div class="seat">13L</div>
        <div class="seat">14L</div>
        <div class="seat">13L</div>
        <div class="seat">14L</div>
        <div class="seat">13L</div>
        <div class="seat">14L</div>
        <div class="seat">13L</div>
        <div class="seat">14L</div>
        <div class="seat">13L</div>
        <div class="seat">14L</div>
      </div>

      <!-- Center aisle (empty space) -->
      <div class="aisle"></div>

      <!-- Right Side Seats -->
      <div class="seats right-side">
        <div class="seat">1R</div>
        <div class="seat">2R</div>
        <div class="seat">3R</div>
        <div class="seat">4R</div>
        <div class="seat">5R</div>
        <div class="seat">6R</div>
        <div class="seat">7R</div>
        <div class="seat">8R</div>
        <div class="seat">9R</div>
        <div class="seat">10R</div>
        <div class="seat">11R</div>
        <div class="seat">12R</div>
        <div class="seat">13R</div>
        <div class="seat">14R</div>
        <div class="seat">15R</div>
        <div class="seat">16L</div>
        <div class="seat">17L</div>
        <div class="seat">18L</div>
        <div class="seat">16L</div>
        <div class="seat">17L</div>
        <div class="seat">18L</div>
        <div class="seat">16L</div>
        <div class="seat">17L</div>
        <div class="seat">18L</div>
        <div class="seat">16L</div>
        <div class="seat">17L</div>
        <div class="seat">18L</div>
        <div class="seat">16L</div>
        <div class="seat">17L</div>
        <div class="seat">18L</div>
      </div>
    </div>
  </div>

  <!-- Footer -->
  <div id="footer"></div>

  <script src="script.js"></script>
</body>
</html>
